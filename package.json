{
  "name": "vue-demi",
  "version": "0.14.7",
  "engines": {
    "node": ">=12"
  },
  "repository": "https://github.com/antfu/vue-demi.git",
  "funding": "https://github.com/sponsors/antfu",
  "license": "MIT",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "main": "dist/index.js",
  "jsdelivr": "dist/index.global.js",
  "unpkg": "dist/index.global.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "browser": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "bin": {
    "vue-demi-fix": "bin/vue-demi-fix.js",
    "vue-demi-switch": "bin/vue-demi-switch.js"
  },
  "files": [
    "dist",
    "bin",
    "scripts"
  ],
  "scripts": {
    "dev": "tsup-node --watch",
    "build": "rimraf dist && tsup-node",
    "postinstall": "node -e \"try{require('./scripts/postinstall.js')}catch(e){}\"",
    "release": "npx bumpp --tag --commit --push && npm publish"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^3.0.0-0 || ^2.6.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@vue/composition-api": "^1.7.2",
    "citty": "^0.1.6",
    "esbuild-plugin-replace": "^1.4.0",
    "eslint": "^8.57.0",
    "jiti": "^1.21.0",
    "mlly": "^1.6.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.2",
    "unimport": "^3.7.1",
    "unplugin-replace": "link:..\\unplugin-replace",
    "vue-v2.0": "npm:vue@2.6.14",
    "vue-v2.7": "npm:vue@2.7.15",
    "vue-v3": "npm:vue@latest"
  }
}
